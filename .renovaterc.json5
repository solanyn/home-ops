{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigests",
    "github>solanyn/home-ops//.renovate/autoMerge.json5",
    "github>solanyn/home-ops//.renovate/changelogs.json5",
    "github>solanyn/home-ops//.renovate/customManagers.json5",
    "github>solanyn/home-ops//.renovate/grafanaDashboards.json5",
    "github>solanyn/home-ops//.renovate/groups.json5",
    "github>solanyn/home-ops//.renovate/labels.json5",
    "github>solanyn/home-ops//.renovate/overrides.json5",
    "github>solanyn/home-ops//.renovate/semanticCommits.json5",
    "github>solanyn/home-ops//.renovate/talosFactory.json5",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":timezone(Australia/Sydney)",
  ],
  "dependencyDashboardTitle": "Renovate Dashboard ðŸ¤–",
  suppressNotifications: [
    "prEditedNotification",
    "prIgnoreNotification",
  ],
  ignorePaths: [
    "**/resources/**",
    "third_party/",
  ],
  flux: {
    managerFilePatterns: ["/\\.yaml(?:\\.j2)?$/"],
  },
  kubernetes: {
    managerFilePatterns: ["/\\.yaml(?:\\.j2)?$/"],
  },
}
